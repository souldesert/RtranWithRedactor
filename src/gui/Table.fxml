<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.Table">
   <children>
      <TableView AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="40.0">
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <columns>
            <TableColumn fx:id="Метка" prefWidth="110.0" sortable="false" text="Метка" />
            <TableColumn fx:id="Условие" prefWidth="112.0" sortable="false" text="Условие" />
            <TableColumn fx:id="ЛинейныеОператоры" prefWidth="75.0" sortable="false" text="Линейные операторы" />
            <TableColumn fx:id="МеткаПерехода" prefWidth="75.0" sortable="false" text="Метка перехода" />
            <TableColumn fx:id="Комментарии" prefWidth="75.0" sortable="false" text="Комментарии" />
         </columns>
      </TableView>
      <ButtonBar prefHeight="40.0" prefWidth="200.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <buttons>
          <Button mnemonicParsing="false" text="Button" />
            <Button mnemonicParsing="false" text="Button" />
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
